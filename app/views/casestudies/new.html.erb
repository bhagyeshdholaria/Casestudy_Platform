<h1>Creating new Casestudy</h1>

<%= form_with model: @casestudy do |f| %>
  <div class="mb-3">
    <%= f.label :name %>
    <%= f.text_field :name, class: 'form-control' %>
  </div>

  <div class="mb-3">
    <%= f.label :duration %>
    <%= f.text_field :duration, class: 'form-control' %>
  </div>

  <div class="mb-3">
    <%= f.label :scale %>
    <%= f.text_field :scale, class: 'form-control' %>
  </div>

  <%= f.button 'Create' %>
<% end %>
